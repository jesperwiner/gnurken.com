{
  "name": "gnurken.com",
  "version": "0.0.1",
  "description": "Gnurken",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "clean": "rimraf dist",
    "build": "npm-run-all clean prod webpack:bundle copy:bundle",
    "prod": "set NODE_ENV=production",
    "copy:bundle": "copy.files.bat",
    "webpack:bundle": "webpack --type bundle --progress --colors --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jesperwiner/gnurken.com"
  },
  "keywords": [
    "react",
    "reactjs"
  ],
  "homepage": "http://www.gnurken.com",
  "license": "MIT",
  "dependencies": {
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-prop-types": "^0.4.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^2.0.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-config-nordnet": "^1.1.1",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.0",
    "npm-run-all": "^1.8.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.5.1",
    "webpack": "^1.13.1",
    "webpack-configurator": "^0.3.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.2.0",
    "webpack-uglify-js-plugin": "^1.1.9"
  }
}
